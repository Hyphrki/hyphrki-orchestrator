# Docker Environment Configuration
# Copy this file to .env.docker.local and customize values

# PostgreSQL
POSTGRES_PASSWORD=orchestrator_secure_password_2025

# N8N Configuration
N8N_USER=admin
N8N_PASSWORD=n8n_admin_password_2025
N8N_DB_NAME=n8n
N8N_DB_USER=orchestrator
N8N_DB_PASSWORD=orchestrator_secure_password_2025
N8N_API_KEY=n8n-api-key-for-orchestrator-integration
N8N_HOST=localhost

# Encryption (Generate with: openssl rand -hex 32)
ENCRYPTION_KEY=0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef

# JWT Secret (Generate with: openssl rand -base64 32)
JWT_SECRET=your-jwt-secret-key-change-in-production

# Auth0 (Get from Auth0 Dashboard)
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_AUDIENCE=https://api.orchestrator.com
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret

# Frontend URLs
ADMIN_FRONTEND_URL=http://localhost:5173
USER_PORTAL_URL=http://localhost:3000

# Hyphrki Marketplace (Optional)
HYPHRKI_WS_URL=wss://hyphrki.com/ws
HYPHRKI_API_URL=https://api.hyphrki.com
